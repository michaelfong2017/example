{
    "name": "react-app",
    "version": "1.0.0",
    "main": "index.js",
    "license": "MIT",
    "private": true,
    "scripts": {
        "dev": "webpack serve --config webpack.config.dev.js",
        "build": "webpack --config webpack.config.prod.js",
        "docker": "docker rm -vf example-web && docker rmi -f example-web && docker build -t example-web . && docker run -d -p 8080:80 --name example-web example-web",
        "publish": "docker tag example-web tsushima:5000/example-web:$npm_package_version && docker push tsushima:5000/example-web:$npm_package_version",
        "deploy": "yarn run build && yarn run docker && yarn run publish"
    },
    "devDependencies": {
        "@babel/core": "^7.12.13",
        "@babel/plugin-transform-runtime": "^7.12.15",
        "@babel/preset-env": "^7.12.13",
        "@babel/preset-react": "^7.12.13",
        "autoprefixer": "^10.1.0",
        "babel-loader": "^8.2.2",
        "babel-plugin-react-html-attrs": "^3.0.5",
        "clean-webpack-plugin": "^3.0.0",
        "copy-webpack-plugin": "^7.0.0",
        "css-loader": "^5.0.1",
        "file-loader": "^6.2.0",
        "html-webpack-plugin": "^4.5.0",
        "node-sass": "^5.0.0",
        "postcss-loader": "^4.1.0",
        "react-hot-loader": "^4.13.0",
        "sass-loader": "^10.1.0",
        "style-loader": "^2.0.0",
        "uglifyjs-webpack-plugin": "^2.2.0",
        "webpack": "^5.11.0",
        "webpack-bundle-analyzer": "^4.4.2",
        "webpack-cli": "^4.2.0",
        "webpack-dev-server": "^3.11.0"
    },
    "dependencies": {
        "@hot-loader/react-dom": "^17.0.1",
        "bootstrap": "^4.6.0",
        "react": "^17.0.1",
        "react-bootstrap": "^1.5.0",
        "react-dom": "^17.0.1",
        "react-router-dom": "^5.2.0",
        "recoil": "^0.1.2",
        "styled-components": "^5.3.0"
    },
    "eslintConfig": {
        "extends": "react-app"
    }
}
